<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yel.myqoutes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Host.Splash"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.Host.Main_Host"></activity>

        <activity
            android:name=".Activity.Host.Content_Category_Host"
            android:label="" />


        <activity android:name=".Activity.Host.More">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.Host.Main_Host" />
        </activity>


        <activity android:name=".Activity.Host.About" />


    </application>

</manifest>
